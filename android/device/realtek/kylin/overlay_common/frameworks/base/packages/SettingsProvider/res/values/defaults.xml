<resources>
    <!-- Set the automatic brightness mode on by default -->
    <bool name="def_screen_brightness_automatic_mode">true</bool>

    <!-- Value for Settings.Global.LOW_BATTERY_SOUND_TIMEOUT:
         Mute battery sounds on tablets after one hour with screen off -->
    <integer name="def_low_battery_sound_timeout">3600000</integer>

    <!-- Comma-separated list of location providers.
         Network location is off by default because it requires
         user opt-in via Setup Wizard or Settings.
    -->
    <string name="def_location_providers_allowed" translatable="false">network</string>

    <!-- Default for Settings.System.REALTEK_SETUP_HDMI_OUTPUT_MODE -->
    <!-- 0:RAW, 1:LPCM 2ch, 2:LPCM Multi-ch, 3:Auto -->
    <add-resource type="string" name="def_realtek_setup_hdmi_ouput_mode"/>
    <string name="def_realtek_setup_hdmi_ouput_mode">1</string>

    <!-- Default for Settings.System.REALTEK_DEEP_COLOR_MODE -->
    <!-- AUTO(0), 12BIT(1), 10BIT(2), OFF(3) -->
    <add-resource type="integer" name="def_realtek_deep_color_mode"/>
    <integer name="def_realtek_deep_color_mode">3</integer>

    <integer name="def_screen_off_timeout">300000</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_DLNA_DMR_ON -->
    <add-resource type="bool" name="def_realtek_setup_dlna_dmr_on"/>
    <bool name="def_realtek_setup_dlna_dmr_on">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_MIRACAST_SINK_TOGGLE_ON -->
    <add-resource type="bool" name="def_realtek_setup_miracast_sink_toggle_on"/>
    <bool name="def_realtek_setup_miracast_sink_toggle_on">false</bool>

    <bool name="def_install_non_market_apps">true</bool>
    <bool name="def_stay_on_while_plugged_in">true</bool>
    <bool name="def_lockscreen_disabled">true</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_TV_SYSTEM -->
    <add-resource type="integer" name="def_realtek_setup_tv_system"/>
    <integer name="def_realtek_setup_tv_system">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_TV_STANDARD -->
    <add-resource type="integer" name="def_realtek_setup_tv_standard"/>
    <integer name="def_realtek_setup_tv_standard">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_REAL_TV_SYSTEM -->
    <add-resource type="integer" name="def_realtek_setup_real_tv_system"/>
    <integer name="def_realtek_setup_real_tv_system">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_TV_SYSTEM_USER -->
    <add-resource type="integer" name="def_realtek_setup_tv_system_user"/>
    <integer name="def_realtek_setup_tv_system_user">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_ASPECT_RATIO -->
	<!-- Default value is FIT_MODE (0) -->
    <add-resource type="integer" name="def_realtek_setup_aspect_ratio"/>
    <integer name="def_realtek_setup_aspect_ratio">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_CURRENT_HDMI_TV_VID -->
    <add-resource type="integer" name="def_realtek_setup_current_hdmi_tv_vid"/>
    <integer name="def_realtek_setup_current_hdmi_tv_vid">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_TV_SYSTEM_AUTO_MODE -->
	<!-- Default value is TV_SYSTEM_AUTO_MODE_HDMI_EDID (2) -->
    <add-resource type="integer" name="def_realtek_setup_tv_system_auto_mode"/>
    <integer name="def_realtek_setup_tv_system_auto_mode">2</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_TV_SYSTEM_MANUAL -->
    <add-resource type="integer" name="def_realtek_setup_tv_system_manual"/>
    <integer name="def_realtek_setup_tv_system_manual">0</integer>


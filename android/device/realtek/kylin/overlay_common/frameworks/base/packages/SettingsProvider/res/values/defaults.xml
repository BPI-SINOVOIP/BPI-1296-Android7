    <add-resource type="bool" name="def_realtek_is_def_device_name_changed_by_user"/>
    <bool name="def_realtek_is_def_device_name_changed_by_user">false</bool>

    <!-- Default for Settings.System.REALTEK_IS_WIFI_MACADDR_READY-->
    <add-resource type="bool" name="def_realtek_is_wifi_macAddr_ready"/>
    <bool name="def_realtek_is_wifi_macAddr_ready">false</bool>

    <!-- Default for Settings.System.REALTEK_DEFAULT_DEVICE_NAME-->
    <add-resource type="string" name="def_realtek_def_deivce_name"/>
    <string name="def_realtek_def_deivce_name">AirFun</string>

    <!-- Default for Settings.System.REALTEK_IGNORE_HDMI_PLUGOUT-->
    <add-resource type="bool" name="def_realtek_ignore_hdmi_plugout"/>
    <bool name="def_realtek_ignore_hdmi_plugout">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_AUTO_1080P24-->
    <add-resource type="bool" name="def_realtek_setup_auto_1080p24"/>
    <bool name="def_realtek_setup_auto_1080p24">true</bool>

    <!-- Default for Settings.System.YST_MODE-->
    <add-resource type="bool" name="def_realtek_yst_mode"/>
    <bool name="def_realtek_yst_mode">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_AUTO_2997HZ-->
    <add-resource type="bool" name="def_realtek_setup_auto_2997hz"/>
    <bool name="def_realtek_setup_auto_2997hz">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_FORCE_SDAUDIO -->
    <add-resource type="bool" name="def_realtek_setup_force_sdaudio"/>
    <bool name="def_realtek_setup_force_sdaudio">false</bool>

    <!-- Default for Settings.System.REALTEK_FORCE_RT_MEDIAPLAYER -->
    <add-resource type="bool" name="def_realtek_force_rt_mediaplayer_on"/>
    <bool name="def_realtek_force_rt_mediaplayer_on">true</bool>

    <!-- Default for Settings.Global.CAPTIVE_PORTAL_DETECTION_ENABLED -->
    <add-resource type="integer" name="def_connectivity_check_enabled"/>
    <integer name="def_connectivity_check_enabled">1</integer>

    <!-- Default for Settings.Global.CAPTIVE_PORTAL_SERVER -->
    <add-resource type="string" name="def_connectivity_check_url"/>
    <string name="def_connectivity_check_url">connectivitycheck.gstatic.com</string>
</resources>

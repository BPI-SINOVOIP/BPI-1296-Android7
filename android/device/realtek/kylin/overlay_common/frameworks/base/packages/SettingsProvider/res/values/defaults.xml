	<!-- Default value is VOUT_YUV (0) -->
    <add-resource type="integer" name="def_realtek_setup_video_output"/>
    <integer name="def_realtek_setup_video_output">0</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_DLNA_DMS_DEVICE_NAME -->
    <add-resource type="string" name="def_realtek_setup_dlna_dms_device_name"/>
    <string name="def_realtek_setup_dlna_dms_device_name" translatable="false">Realtek_Device</string>


    <!-- Default for Settings.System.REALTEK_SETUP_PLAYBACK_EFFECT_MODE -->
    <add-resource type="string" name="def_realtek_setup_playback_effect_mode"/>
    <string name="def_realtek_setup_playback_effect_mode">off</string>

    <!-- Default for Settings.System.REALTEK_SETUP_SOFTAP_WPS_METHOD -->
    <add-resource type="string" name="def_realtek_setup_softap_wps_method"/>
    <string name="def_realtek_setup_softap_wps_method">sw_pbc</string>

    <!-- Default for Settings.Global.HDMI_CEC_POWER_ON_FROM_TV -->
    <add-resource type="integer" name="def_hdmi_cec_power_on_from_tv"/>
    <integer name="def_hdmi_cec_power_on_from_tv">1</integer>

    <!-- Default for Settings.System.REALTEK_SETUP_IRDA_CEC_SWITCH -->
    <add-resource type="string" name="def_realtek_setup_irda_cec_switch"/>
    <string name="def_realtek_setup_irda_cec_switch">off</string>

    <!-- Default for Settings.System.REALTEK_SETUP_SCREEN_ZOOM_ON -->
    <add-resource type="bool" name="def_realtek_setup_screen_zoom_on"/>
    <bool name="def_realtek_setup_screen_zoom_on">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_USER_HAVE_AMPLIFIER_ON -->
    <add-resource type="bool" name="def_realtek_setup_user_have_amplifier_on"/>
    <bool name="def_realtek_setup_user_have_amplifier_on">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_USER_HAVE_AMPLIFIER_51CH_OUTPUT_ON -->
    <add-resource type="bool" name="def_realtek_setup_user_have_amplifier_51ch_output_on"/>
    <bool name="def_realtek_setup_user_have_amplifier_51ch_output_on">false</bool>

    <!-- Default for Settings.System.REALTEK_SETUP_USER_HAVE_AMPLIFIER_71CH_OUTPUT_ON -->
    <add-resource type="bool" name="def_realtek_setup_user_have_amplifier_71ch_output_on"/>
    <bool name="def_realtek_setup_user_have_amplifier_71ch_output_on">false</bool>


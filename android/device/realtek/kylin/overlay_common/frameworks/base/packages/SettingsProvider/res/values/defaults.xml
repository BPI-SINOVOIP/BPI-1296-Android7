    <add-resource type="string" name="def_realtek_setup_spdif_ouput_mode"/>
    <string name="def_realtek_setup_spdif_ouput_mode">0</string>

    <!-- Default for Settings.System.REALTEK_SETUP_TEXT_ENCODING_MODE -->
    <add-resource type="string" name="def_realtek_setup_text_encoding_mode"/>
    <string name="def_realtek_setup_text_encoding_mode">AUTO</string>

    <!-- Default for Settings.System.REALTEK_SETUP_CGI_UPLOAD_FILE_PATH -->
    <add-resource type="string" name="def_realtek_setup_cgi_upload_file_path"/>
    <string name="def_realtek_setup_cgi_upload_file_path">/data/upload</string>

    <!-- Default for Settings.System.REALTEK_SETUP_MIRACAST_SINK_CONNECTION_REMINDER_ON -->
    <add-resource type="bool" name="def_realtek_setup_miracast_sink_connection_reminder_on"/>
